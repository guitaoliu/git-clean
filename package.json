{
  "name": "@maccuaa/git-clean",
  "version": "1.0.5",
  "description": "CLI tool for finding stale Git branches",
  "main": "bin/git-clean",
  "preferGlobal": true,
  "bin": {
    "git-clean": "bin/git-clean.js"
  },
  "scripts": {
    "lint": "eslint --ext mjs ./src",
    "build": "rimraf dist && npm run lint && babel src --out-dir dist",
    "prepublishOnly": "npm run lint && npm run build",
    "start": "babel-node src/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maccuaa/git-clean.git"
  },
  "engines": {
    "node": ">=10"
  },
  "author": "Andrew MacCuaig <andrewmaccuaig@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/node": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/preset-env": "7.10.4",
    "babel-eslint": "10.1.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "prettier": "2.0.5",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "inquirer": "7.2.0",
    "ora": "4.0.4",
    "rc": "1.2.8",
    "shelljs": "0.8.4",
    "url-join": "4.0.1"
  }
}
